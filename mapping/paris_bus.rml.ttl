@prefix : <http://example.org/rules/> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix eona: <https://raw.githubusercontent.com/Max-Bld/eonax_ontology_prototype/main/eona_x_testing_ontology_2#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix zenbus: <http://zenbus.net/> .
@prefix gtfs: <http://vocab.gtfs.org/terms#> .

:TriplesMap a rr:TriplesMap;
  rml:logicalSource [
    rml:source [
      a csvw:Table;
      csvw:url "/home/maximeb/github/datasets_alignment/datasets/paris_bus.csv";
      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV
  ].

:TriplesMap rr:subjectMap [
    rr:template "http://zenbus.net/{shape_id}"
    ].

:TriplesMap rr:predicateObjectMap [
  rr:predicate rdf:type;
  rr:objectMap [ rr:constant eona:Shape ];
].

:TriplesMap rr:predicateObjectMap [
  rr:predicate geo:lat;
  rr:objectMap [ rml:reference "shape_pt_lat" ];
].

:TriplesMap rr:predicateObjectMap [
  rr:predicate geo:lon;
  rr:objectMap [ rml:reference "shape_pt_lon" ];
].

